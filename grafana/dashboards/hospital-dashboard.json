      {
        "id": 7,
        "title": "Recent Appointments",
        "type": "table",
        "targets": [
          {
            "datasource": {
              "type": "postgres",
              "uid": "hospital-postgres"
            },
            "format": "table",
            "rawSql": "SELECT \n  a.appointment_time as \"Appointment Time\",\n  d.full_name as \"Doctor\",\n  d.specialization as \"Specialization\",\n  p.full_name as \"Patient\",\n  a.status as \"Status\"\nFROM appointments a\nJOIN doctors d ON a.doctor_id = d.id\nJOIN patients p ON a.patient_id = p.id\nORDER BY a.appointment_time DESC\nLIMIT 10;",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 9,
          "w": 24,
          "x": 0,
          "y": 17
        },
